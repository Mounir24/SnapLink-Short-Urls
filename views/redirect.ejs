        <script type="text/javascript">
            // REDIRECT COUNTER 

                let count = 10;
                
                function counter() {
                    let counter = document.querySelector('.redirect__count');
                    count--;
                    counter.innerText = count;
                    if (count <= 0) {
                        clearInterval(stop);
                        window.location = "<%= url %>";
                    }
                }
                const stop = setInterval(counter, 1000);
        </script>
